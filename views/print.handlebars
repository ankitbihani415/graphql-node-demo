    .hiw {
      overflow: hidden;
      margin-bottom: 3em;
    }

    .hiw article {
      position: relative;
      text-align: center;
      background: #EEEEEE;
    }

    .hiw article span {
      display: inline-block;
      padding: 0.75em;
    }

    .hiw article:hover h3 {
      color: #ff6600 !important;
    }

    .hiw article:before {
      top: 100%;
      left: 0;
      width: 100%;
      height: 100%;
      content: "";
      position: absolute;
      background: inherit;
    }

    .hiw article:after {
      top: 50%;
      left: 100%;
      width: 0;
      height: 0;
      z-index: 2;
      content: "";
      position: absolute;
      margin-top: -0.75em;
      border-top: 1.5em solid transparent;
      border-left: 1.5em solid #EEEEEE;
      border-bottom: 1.5em solid transparent;
    }

    .hiw article:nth-child(2) {
      background: #333333;
    }

    .hiw article:nth-child(2) h3 {
      color: white;
    }

    .hiw article:nth-child(2):after {
      border-left-color: #333333;
    }

    .hiw article:nth-child(3) {
      background: #ff944d;
    }

    .map {
      width: 100%;
      min-height: 340px;
    }

    .campaing-map {
      position: relative;
    }

    .campaing-map-container {
      width: 100%;
      position: absolute;
    }

    .campaign-form .estimated-impressions {
